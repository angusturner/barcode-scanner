# Claude API Key (optional if using the web UI to input it)
# To set this in Firebase: firebase functions:config:set claude.api_key="your-key-here"
CLAUDE_API_KEY=

# Claude API Proxy URL (for local development, set to your localhost URL)
# VITE_CLAUDE_PROXY_URL=http://localhost:3001/api/claude

# Firebase Project ID (needed for emulator)
VITE_FIREBASE_PROJECT_ID=your-project-id

# Enable Firebase Emulator for local development
VITE_USE_EMULATOR=true

# Firebase Configuration (optional, only needed if using Firebase authentication or other services)
VITE_FIREBASE_API_KEY=
VITE_FIREBASE_AUTH_DOMAIN=
VITE_FIREBASE_STORAGE_BUCKET=
VITE_FIREBASE_MESSAGING_SENDER_ID=
VITE_FIREBASE_APP_ID= 